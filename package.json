    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.8.0",
    "chai": "^3.5.0",
    "css-loader": "^0.26.1",
    "eslint": "^2.9.0",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-react": "^6.9.0",
    "eslint-watch": "^2.1.11",
    "mocha": "^3.2.0",
