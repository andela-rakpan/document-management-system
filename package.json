  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andela-rakpan/document-management-system/issues"
  },
  "homepage": "https://github.com/andela-rakpan/document-management-system#readme",
  "dependencies": {
    "body-parser": "^1.16.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "express": "^4.14.0",
    "morgan": "^1.7.0",
    "open": "0.0.5",
    "sequelize": "^3.30.0",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.15.0"
  },
  "devDependencies": {
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-loader": "6.2.4",
