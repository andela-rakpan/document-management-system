{
  "name": "document-management-system",
  "version": "1.0.0",
  "description": "A full stack document management system, complete with roles and privileges. Each document defines access rights; the document defines which roles can access it. Also, each document specifies the date it was published.",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist/ && babel ./ --copy-files -d dist --presets es2015,stage-2  --ignore ./node_modules,client,coverage",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "db:migrate": "sequelize db:migrate",
    "seed:test": "NODE_ENV=test babel-node test/server/seedHelper.js",
    "seed:db": "node dist/test/server/seedHelper.js",
    "start": "npm-run-all build seed:db start:server",
    "start:server": "node dist/lib/devServer.js",
    "start:nodemon": "nodemon lib/devServer.js --exec babel-node --presets es2015,stage-2",
    "test:server": "NODE_ENV=test babel-node ./node_modules/babel-istanbul/lib/cli.js cover ./node_modules/.bin/_mocha test/server/**/*.test.js",
    "test": "npm-run-all seed:test test:server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-rakpan/document-management-system.git"
  },
  "keywords": [],
  "author": "Raphael Akpan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andela-rakpan/document-management-system/issues"
  },
  "homepage": "https://github.com/andela-rakpan/document-management-system#readme",
  "dependencies": {
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-istanbul": "^0.12.1",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "6.8.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.16.0",
    "coveralls": "^2.11.16",
    "dotenv": "^4.0.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "faker": "^3.1.0",
    "fm-log": "^4.4.3",
    "jsonwebtoken": "^7.2.1",
    "morgan": "^1.7.0",
    "npm-run-all": "^4.0.1",
    "pg": "^6.1.2",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.30.2",
    "sequelize-cli": "^2.5.1",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.15.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "css-loader": "^0.26.1",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-react": "^6.9.0",
    "eslint-watch": "^2.1.11",
    "mocha": "^3.2.0",
    "node-sass": "^3.13.1",
    "path": "^0.12.7",
    "sass-loader": "^4.1.1",
    "supertest": "^3.0.0"
  }
}
