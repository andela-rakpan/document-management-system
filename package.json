{
  "name": "document-management-system",
  "version": "1.0.0",
  "description": "A full stack document management system, complete with roles and privileges. Each document defines access rights; the document defines which roles can access it. Also, each document specifies the date it was published.",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel start:server test:watch",
    "start:server": "babel-node tools/devServer.js",
    "test:server": "./node_modules/.bin/_mocha tools/testServer.js server/test/**/*.test.js",
    "test:client": "./node_modules/.bin/_mocha tools/testClient.js client/test/**/*.test.js",
    "test": "npm-run-all test:server",
    "test:watch": "npm run test:server -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-rakpan/document-management-system.git"
  },
  "keywords": [],
